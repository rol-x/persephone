{
  "family": "persephone",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::222917425484:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "persephone",
      "image": "222917425484.dkr.ecr.eu-north-1.amazonaws.com/persephone",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8084,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/persephone",
          "awslogs-region": "eu-north-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
